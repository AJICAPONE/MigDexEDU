@font-face {
  font-family: 'Open Sans EB';
  src: local('Open Sans Extra Bold'), local('Open-Sans-Extra-Bold'),
          url('/static/fonts/OpenSans-ExtraBold.woff2') format('woff2'),
          url('/static/fonts/OpenSans-ExtraBold.woff') format('woff'),
          url('/static/fonts/OpenSans-ExtraBold.ttf') format('truetype');
  font-weight: 800;
  font-style: normal;
}
