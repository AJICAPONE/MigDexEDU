.menu-sandwich {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  width: 50px;
  height: 50px;
}

.menu-sandwich .hamburguer {
  position: relative;
}

.menu-sandwich .hamburguer,
.menu-sandwich .hamburguer:before,
.menu-sandwich .hamburguer:after {
  display: block;
  background-color: #fff;
  width: 30px;
  height: 4px;
  transition: all 0.3s ease-in-out;
}

.menu-sandwich .hamburguer:before,
.menu-sandwich .hamburguer:after {
  content: '';
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}

.menu-sandwich .hamburguer:before {
  top: -10px;
}

.menu-sandwich .hamburguer:after {
  bottom: -10px;
}


.menu-sandwich.active .hamburguer {
  bottom: 0;
  transform: rotate(90deg);
  width: 0;
}

.menu-sandwich.active .hamburguer:before {
  top: 0;
  transform: translateX(-50%) rotate(135deg);
}

.menu-sandwich.active .hamburguer:after {
  bottom: 0;
  transform: translate(-50%) rotate(45deg);
}
