.js-index-slider-dots--wrap
  absolute($right:0,$left:0,$bottom:25px)
  width: 100%;
  height: 30px;
  pointer-events none

  ul
    display flex
    justify-content center
    align-items center

  li
    size(24px)
    display: flex;
    align-items: center;
    justify-content: center;
    border: 1.5px solid transparent;
    border-radius: 50%
    transition: all linear 0.2s;
    transform: scale(1);
    margin-right: 10px;
    cursor pointer
    pointer-events auto


li.slick-active .js-index-slider__dots
  background: linear-gradient(135deg, #f37335, #fdc830);
  transition all linear .4s
  position absolute
  opacity 1
  box-shadow: 0 -3px rgba(255, 255, 255, 0) inset, 0 0 15px rgba(253, 200, 48, 0.53);

  &:before
    content ''
    position absolute
    left -4px
    top -4px
    background-color transparent
    size(20px)
    border 1.2px solid #F58434
    border-radius 50%

.js-index-slider__dots
  size(12px)
  display block
  list-style: none;
  border-radius: 50px;
  background-color #FFFFFF
  opacity 0.5
  transition: all linear 0.2s;

.index-slider-content-test
  display flex
  flex-direction column
  align-items flex-end

.index-slider-content-test--wrap
  position relative
  display flex
  flex-direction column
  align-items center
  width max-content
  margin-top 10px

  .icon-Arrow
    position: absolute;
    top: 60px;
    right: 15px;
    size(91px,85px)

.index-slider-content-test-button
  position absolute
  top: 100px;
  right: -90px;


  a
    background: linear-gradient(92.29deg, rgba(243, 117, 53, 0.8) 0%, rgba(253, 198, 48, 0.8) 100%);
    size(74px)
    display flex
    justify-content center
    align-items center
    border-radius 50%
    outline none

    &:before
      content ''
      width: 180%;
      height: 180%;
      background-color: transparent;
      border: 1.5px solid rgba(243, 117, 53, 0.8);
      animation: ajicacircul 2.2s infinite ease-in-out;
      transform-origin: 50% 50%;
      left: calc(-40%);
      top: calc(-40%);
      animation-delay: 2s;
      position: absolute;
      border-radius: 100%;
      transition: all .5s;
      box-sizing: border-box;
      opacity: 0;

    &:after
      content ''
      width: 130%;
      height: 130%;
      background: linear-gradient(92.29deg, rgba(243, 117, 53, 0.8) 0%, rgba(253, 198, 48, 0.8) 100%);
      border: 2px solid transparent;
      animation: ajicacircul2 2.2s infinite ease-in-out;
      background: linear-gradient(92.29deg, rgba(243, 117, 53, 0.8) 0%, rgba(253, 198, 48, 0.8) 100%);
      left: calc(-15%);
      top: calc(-15%);
      position: absolute;
      border-radius: 100%;
      transition: all .5s;
      box-sizing: border-box;
      opacity: 0;
      animation-delay: 2.5s;

    @keyframes ajicacircul
      0%
        transform: rotate(0) scale(.5) skew(1deg);
        opacity: .01;
      30%
        transform: rotate(0) scale(.7) skew(1deg);
        opacity: .5;
      100%
        transform: rotate(0) scale(1) skew(1deg);
        opacity: .01;

    @keyframes ajicacircul2
      0%
        transform: rotate(0) scale(.6) skew(1deg);
        opacity: 0;
      30%
        transform: rotate(0) scale(1) skew(1deg);
        opacity: .2;
      100%
        transform: rotate(0) scale(.6) skew(1deg);
        opacity: .2;


